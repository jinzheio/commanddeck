{
  "name": "desktop",
  "version": "0.1.0",
  "private": true,
  "main": "src/main/main.js",
  "scripts": {
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite src/renderer --port 5173",
    "dev:electron": "wait-on http://localhost:5173 && electron . --dev",
    "build": "vite build src/renderer && electron-builder",
    "typecheck": "tsc --noEmit",
    "postinstall": "electron-rebuild -f -w node-pty"
  },
  "dependencies": {
    "node-pty": "^1.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zustand": "^4.5.0",
    "framer-motion": "^11.0.0",
    "react-diff-viewer-continued": "^3.4.0",
    "clsx": "^2.1.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "autoprefixer": "^10.4.0",
    "concurrently": "^8.2.0",
    "electron": "^31.0.2",
    "electron-rebuild": "^3.2.9",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.1.0",
    "wait-on": "^7.2.0"
  }
}
